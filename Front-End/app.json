{
  "expo": {
    "name": "Simple Chat App",
    "slug": "simple-chat-app",
    "privacy": "public",
    "sdkVersion": "39.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.one.chat"
    },
    "description": "Simple Chat App, based on Now UI Design System. Coded by Sergey & Dzhamil",
    "android": {
      "package": "com.one.chat"
    }
  }
}
